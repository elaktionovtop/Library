<Window x:Class="Library.Views.BookCopiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library.Views"
        xmlns:viewModels="clr-namespace:Library.ViewModels"
       mc:Ignorable="d"
        Title="Экземляры книг" Height="450" Width="800">
    <Window.DataContext>
        <viewModels:BookCopiesViewModel/>
    </Window.DataContext>
    <StackPanel>
        <Menu>
            <MenuItem Header="_Книги" Command="{Binding BooksCommand}"/>
        </Menu>
        <Grid>

        </Grid>
    </StackPanel>
</Window>
